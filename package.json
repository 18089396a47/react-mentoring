{
  "name": "react-mentoring",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "server": "node ./bin/www",
    "start": "webpack-dev-server",
    "build": "webpack -p"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "mapCoverage": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "coveragePathIgnorePatterns": [
      "./setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "jade": "~1.11.0",
    "morgan": "~1.8.1",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "~2.4.2",
    "themoviedb-javascript-library": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "jest": "^21.2.1",
    "jest-css-modules": "^1.1.0",
    "raf": "^3.4.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^16.0.0",
    "style-loader": "^0.18.2",
    "stylus": "0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  }
}
